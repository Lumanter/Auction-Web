<%- include('../partials/header'); %>

<div class="container">
    <div class="form-group row mt-2">
        <h2 class="ml-2">Users</h2>
        <a class="btn btn-outline-primary btn-lg mx-3" href="/users/new">add</a>
    </div>

    <ul class="list-group">
        <% users.forEach((shownUser) => { %>
            <a href="/users/<%= shownUser.id %>" class="list-group-item list-group-item-action"><%= shownUser.nickname %></a>
        <% }); %>
    </ul>
</div>

<%- include('../partials/footer'); %>