<%- include('../partials/header'); %>

<div class="container">
    <header class="jumbotron">
        <h2>Users</h2>
        <a class="btn btn-primary btn-lg" href="/users/new">add</a>
    </header>

    <% if (Object.keys(users).length !== 0) { %>
        <ul class="list-group">
            <% users.forEach((shownUser) => { %>
                <a href="/users/<%= shownUser.id %>" class="list-group-item list-group-item-action"><%= shownUser.nickname %></a>
            <% }); %>
        </ul>
    <% } %>
</div>

<%- include('../partials/footer'); %>